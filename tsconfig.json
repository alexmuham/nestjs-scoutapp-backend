{
  "compilerOptions": {
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "jsx": "react-native",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": "./src",
    "paths": {
//      "resources/*": ["./resources/*"],
//      "api": ["./src/api"],
//      "api/*": ["./src/api/*"],
//      "navigation": ["./src/navigation"],
//      "navigation/*": ["./src/navigation/*"],
//      "components": ["./src/components"],
//      "components/*": ["./src/components/*"],
//      "hooks": ["./src/hooks"],
//      "hooks/*": ["./src/hooks/*"],
//      "entities/*": ["./src/entities/*"],
//      "screens": ["./src/screens"],
//      "screens/*": ["./src/screens/*"],
//      "state": ["./src/state"],
//      "state/*": ["./src/state/*"],
//      "utils": ["./src/utils"],
//      "utils/*": ["./src/utils/*"],
//      "auth": ["./src/auth"],
//      "auth/*": ["./src/auth/*"],
//      "services": ["./src/services"],
//      "services/*": ["./src/services/*"],
    }
  },
  "exclude": [
    "node_modules",
    "build",
    "scripts",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "include": [
    "src"
  ]
}
