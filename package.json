{
  "name": "scoutapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "yarn update-config && react-native start",
    "build": "react-app-rewired build",
    "compile": "yarn update-config && tsc -p tsconfig.json --noEmit",
    "eject": "react-scripts eject",
    "test": "jest",
    "lint": "eslint './**/*.{ts,tsx}'",
    "update-config": "ts-node --project scripts/tsconfig.json scripts/update-config.ts",
    "update-localization": "node scripts/update-localization.js",
    "build:production": "env-cmd -f .env env-cmd -f .env.production react-app-rewired build",
    "build:staging": "env-cmd -f .env env-cmd -f .env.staging react-app-rewired build",
    "docker:build": "docker-compose build",
    "docker:start": "docker-compose up --build",
    "docker:stop": "docker-compose down",
    "docker:push": "sh scripts/push-docker.sh",
    "graphql-generate": "graphql-codegen"
  },
  "dependencies": {
    "@ptomasroos/react-native-multi-slider": "^2.2.2",
    "@react-native-community/async-storage": "^2.0.0-rc.3",
    "@react-native-community/async-storage-backend-legacy": "^2.0.0-rc.3",
    "@redux-saga/core": "^1.1.3",
    "@spryrocks/config-react": "^1.0.0-alpha.7",
    "@spryrocks/react-api": "^0.1.3",
    "@spryrocks/react-auth": "^0.1.4",
    "apollo-boost": "^0.4.7",
    "dotenv": "^8.2.0",
    "firebase": "^7.14.2",
    "fs": "^0.0.1-security",
    "graphql": "15.0.0",
    "history": "^4.10.1",
    "i18next": "^19.4.4",
    "prettier": "^2.0.5",
    "promise-queue": "^2.2.5",
    "ramda": "^0.27.0",
    "react": "16.11.0",
    "react-i18next": "^10.13.1",
    "react-native": "0.62.2",
    "react-native-device-info": "^5.5.7",
    "react-native-fast-image": "^8.1.5",
    "react-native-image-crop-picker": "^0.31.1",
    "react-native-localize": "^1.4.0",
    "react-native-snackbar": "^2.2.0",
    "react-native-status-bar-height": "^2.5.0",
    "react-native-switch": "^1.5.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-native": "^5.1.2",
    "react-scripts": "^3.1.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.1.3",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3",
    "yup": "0.28.5",
    "@spryrocks/dropdown-react-native": "^0.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@graphql-codegen/cli": "^1.10.0",
    "@graphql-codegen/typescript": "^1.10.0",
    "@spryrocks/eslint-config-react": "^0.1.4",
    "@types/node": "^13.13.5",
    "@types/promise-queue": "^2.2.0",
    "@types/ramda": "^0.27.4",
    "@types/react": "^16.9.34",
    "@types/react-native": "^0.62.7",
    "@types/react-redux": "^7.1.8",
    "@types/react-router-native": "^5.1.0",
    "@types/redux-actions": "^2.6.1",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "env-cmd": "^10.1.0",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "16.11.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn compile && yarn lint"
    }
  }
}
